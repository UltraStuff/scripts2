local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
getgenv().SecureMode = true

local Window = Rayfield:CreateWindow({
	Name = "Pixel Piece - Furk Ultra",
	LoadingTitle = "Pet Simulator X - Furk Ultra",
	LoadingSubtitle = "Subscribe to Furky on YouTube!",
})
local Tab = Window:CreateTab("Main GUI's")
local Section = Tab:CreateSection("Pixel Piece X Main GUI's")

local Label = Tab:CreateLabel("Thanks for using Furk Ultra!")



local Button = Tab:CreateButton({
	Name = "Hyper Hub (New)",
	Callback = function()
		pcall(function()
    repeat wait() until game:IsLoaded()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DookDekDEE/Hyper/main/script.lua"))()
end)
	end,
})
local Button = Tab:CreateButton({
	Name = "Gray GUI",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/KodaLINEN/PixelPiece/main/Main'))()
	end,
})
